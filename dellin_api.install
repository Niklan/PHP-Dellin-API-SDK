<?php

/**
 * @file
 * Install, update and uninstall functions for the Delovye Linii API module.
 */

use Drupal\dellin_api\Session\StateStorage;

/**
 * Implements hook_uninstall().
 */
function dellin_api_uninstall() {
  /** @var \Drupal\Core\State\StateInterface $state */
  $state = \Drupal::state();
  $state->delete(StateStorage::STORAGE_KEY);
}
